		<classes>
			<class name="com.TaxSlayer.testClasses.LoginPageExecution" />
			<class name="com.TaxSlayer.testClasses.UserConsoleExecution" />
			<class name="com.TaxSlayer.testClasses.MessageCenterExecution" />
			<class name="com.TaxSlayer.testClasses.ViewDetailsExecution" />
			<class name="com.TaxSlayer.testClasses.TransHistPageExecution" />
			<class name="com.TaxSlayer.testClasses.TrackRefundStatusExecution" />
			<class name="com.TaxSlayer.testClasses.SecurelyIDPackageDetailsExecution" />
			<class name="com.TaxSlayer.testClasses.SecurelyIDOfferExecution" />
			<class name="com.TaxSlayer.testClasses.SecurelyIDEnrollExecution" />
			<class name="com.TaxSlayer.testClasses.MobileAppSettingsExecution" />
			<class name="com.TaxSlayer.testClasses.HelpandSupportTechQueExecution" />
			<class name="com.TaxSlayer.testClasses.HelpandSupportTaxExecution" />
			<class name="com.TaxSlayer.testClasses.HelpandSupportExecution" />
			<class name="com.TaxSlayer.testClasses.HelpandSupportArticlePageExecution" />		  
  			<class name="com.TaxSlayer.testClasses.HurdlrWarningExecution" />
		</classes>
	</test>

	<test name="QAEnvironmentTestDataRequired" enabled='false' thread-count="8">
		<parameter name="QAENVURL1" value="https://ps-id.taxslayer.com/myaccount/" />
		<classes>
			<class name="com.TaxSlayer.testClasses.ChargeSuccessExecution" />
			<class name="com.TaxSlayer.testClasses.ChargeCancelledExecution" />
			<class name="com.TaxSlayer.testClasses.CallChargeAppExecution" />
