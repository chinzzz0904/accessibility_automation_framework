			<class name="com.TaxSlayer.testClasses.PoliciesPrivacyPolicyExecution" />
			<class name="com.TaxSlayer.testClasses.PoliciesLicenseAgreementExecution" />
			<class name="com.TaxSlayer.testClasses.ChargeSuccessExecution" />
		</classes>
	</test>

	<test name="LocalEnvironmentTest" enabled='false' thread-count="2">
		<parameter name="LOCALENVURL" value="http://localhost:3000/myaccount/" />
		<classes>
			<class name="com.TaxSlayer.testClasses.SessionTimeoutPopupExecution" />
			<class name="com.TaxSlayer.testClasses.DiscountOfferExecution" />
		</classes>
	</test>

